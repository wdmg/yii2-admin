{
  "name": "wdmg/yii2-admin",
  "description": "Administrative panel for Butterfly.CMS",
  "keywords": ["yii2", "yii2-admin", "dashboard", "wdmg"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "1.1.20",
  "homepage": "https://github.com/wdmg/yii2-admin",
  "support": {
    "source": "https://github.com/wdmg/yii2-admin",
    "issues": "https://github.com/wdmg/yii2-admin/issues"
  },
  "authors": [
    {
      "name": "Alexsander Vyshnyvetskyy",
      "email": "alex.vyshnyvetskyy@gmail.com"
    },
    {
      "name": "W.D.M.Group, Ukraine",
      "email": "wdmg.com.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.20",
    "wdmg/yii2-base": "^1.1.0"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "wdmg\\admin\\": ""
    }
  },
  "extra": {
    "bootstrap": "wdmg\\admin\\Bootstrap",
    "options": {
      "routePrefix": "admin",
      "checkForUpdates": true,
      "cacheExpire": 3600,
      "rememberDuration": 86400,
      "resetTokenExpire": 3600,
      "supportEmail": "noreply@example.com"
    }
  }
}