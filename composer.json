{
  "name": "wdmg/yii2-admin",
  "description": "Administrative dashboard for Butterfly.CMS",
  "keywords": ["yii2", "yii2-admin", "dashboard", "wdmg"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "1.3.1",
  "homepage": "https://github.com/wdmg/yii2-admin",
  "support": {
    "source": "https://github.com/wdmg/yii2-admin",
    "issues": "https://github.com/wdmg/yii2-admin/issues"
  },
  "authors": [
    {
      "name": "Alexsander Vyshnyvetskyy",
      "email": "alex.vyshnyvetskyy@gmail.com"
    },
    {
      "name": "W.D.M.Group, Ukraine",
      "email": "wdmg.com.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.40",
    "wdmg/yii2-base": "^1.3.2",
    "wdmg/yii2-validators": "^1.0.7",
    "wdmg/yii2-helpers": "^1.4.6",
    "wdmg/yii2-widgets": "^1.0.4",
    "bower-asset/font-awesome": "^5.13"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "wdmg\\admin\\": ""
    }
  },
  "extra": {
    "bootstrap": "wdmg\\admin\\Bootstrap",
    "options": {
      "routePrefix": "admin",
      "checkForUpdates": true,
      "cacheExpire": 3600,
      "rememberDuration": 86400,
      "resetTokenExpire": 3600,
      "supportEmail": "noreply@example.com",
      "menuShowDisabled": false,
      "multiSignIn": false,
      "sessionTimeout": 900,
      "customLocales": [],
      "customSupportModules": [],
      "customSidebarMenu": [],
      "customCreateMenu": [],
      "useSphinxSearch": true,
      "sphinxSearchConf": {
        "dsn": "mysql",
        "host": "127.0.0.1",
        "port": "9306",
        "username": "",
        "password": ""
      }
    }
  }
}